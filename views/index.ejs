                <!-- JavaScript to Handle View More Functionality -->
                <script>
                    document.addEventListener('DOMContentLoaded', function () {
                        const viewMoreBtn = document.getElementById('viewMoreBtn');
                        const hiddenMembers = document.querySelectorAll('.hidden-team-member');
                        let isExpanded = false;

                        viewMoreBtn.addEventListener('click', function () {
                            if (!isExpanded) {
                                hiddenMembers.forEach(member => member.style.display = 'block');
                                viewMoreBtn.textContent = 'View Less';
                                isExpanded = true;
                            } else {
                                hiddenMembers.forEach(member => member.style.display = 'none');
                                viewMoreBtn.textContent = 'View More';
                                isExpanded = false;
                                // Scroll back to the View More button after collapsing
                                viewMoreBtn.scrollIntoView({ behavior: 'smooth' });
                            }
                        });
                    });
